{
  "dashboard": {
    "title": "BondTrader Overview",
    "tags": ["bondtrader", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Requests Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bondtrader_api_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "API Request Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(bondtrader_api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Bond Valuations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bondtrader_valuations_total[5m])",
            "legendFormat": "{{bond_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "ML Predictions",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bondtrader_ml_predictions_total[5m])",
            "legendFormat": "{{model_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bondtrader_cache_hits_total[5m]) / (rate(bondtrader_cache_hits_total[5m]) + rate(bondtrader_cache_misses_total[5m]))",
            "legendFormat": "Hit Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Active Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "bondtrader_active_connections",
            "legendFormat": "Connections"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 1
  }
}
